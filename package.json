{
  "name": "ndjson-preview",
  "displayName": "NDJSON Preview",
  "description": "Preview pretty-printed JSON from NDJSON files line by line",
  "version": "1.0.2",
  "author": {
    "name": "Seydi Ahmet Kiyak",
    "email": "s.ahmet.kiyak@gmail.com"
  },
  "contributors": [
    {
      "name": "Andrew Sidhu",
      "email": "code@kozyops.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahi/ndjson-preview.git"
  },
  "bugs": {
    "url": "https://github.com/ahi/ndjson-preview/issues"
  },
  "homepage": "https://github.com/ahi/ndjson-preview#readme",
  "keywords": [
    "ndjson",
    "jsonl",
    "json",
    "preview",
    "viewer",
    "json lines",
    "newline delimited json"
  ],
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ndjson-preview.open",
        "title": "Open NDJSON Preview"
      }
    ],
    "languages": [
      {
        "id": "jsonl",
        "aliases": [
          "NDJSON",
          "JSON Lines"
        ],
        "extensions": [
          ".ndjson",
          ".jsonl"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./ && pnpm run copy-vendor",
    "copy-vendor": "mkdir -p out/vendor/pretty-print-json && cp node_modules/pretty-print-json/dist/css/pretty-print-json.css out/vendor/pretty-print-json/ && cp node_modules/pretty-print-json/dist/pretty-print-json.min.js out/vendor/pretty-print-json/",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vitest run",
    "test:unit": "vitest run src/test/unit",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "package": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/js": "^10.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^25.3.1",
    "@types/vscode": "^1.109.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^10.0.2",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "pretty-print-json": "^3.0.6"
  }
}
